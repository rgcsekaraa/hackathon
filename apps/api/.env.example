# Spatial Voice API -- Environment Variables
# Copy this file to .env and fill in your values.

# OpenRouter LLM (required for AI features, optional for dev -- mock parser works without it)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=anthropic/claude-sonnet-4

# Redis (optional -- caching degrades gracefully without it)
REDIS_ENABLED=false
REDIS_URL=redis://localhost:6379

# Server
DEBUG=true
