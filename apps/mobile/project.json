{
  "name": "mobile",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx expo start",
        "cwd": "apps/mobile"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx expo export -p web",
        "cwd": "apps/mobile"
      }
    }
  }
}
